      <!-- Modal content-->
      <div class="modal-content">
        <% form_for :feed, url(:feed, :create), :class => 'form-horizontal' do |f| %>
	  <%= hidden_field_tag(:origin, value: @origin) %>

          <div class="modal-header">
            <h4 class="modal-title">Feed</h4>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
	    </button>
          </div>

          <div class="modal-body">
	    <div class="form-group row">
	      <label for="rss_url" class="col-sm-2 col-form-label">URL:</label>
	      <div class="col-sm-10">
		<input type="text" class="form-control" name="rss_url">
	      </div>
	    </div>

	    <div class="form-group row">
	      <table class="use_log_flags col-sm">
		<tr>
		  <th></th>
		  <t>use</td>
		    <td>log</td>
		    <td>log words</td>
		  </tr>
		</theader>
		<tbody>
		  <tr>
		    <td>body</>
		    <td><%= check_box_tag :use_body, checked: @feed[:use_body] %></td>
	      	    <td><%= check_box_tag :log_body, checked: @feed[:log_body] %></td>
		    <td><%= check_box_tag :log_body_words, checked: @feed[:log_body_words] %></td>
		  </tr>
		  <tr>
		    <td>description</td>
		    <td><%= check_box_tag :use_description, checked: @feed[:use_description] %></td>
		    <td><%= check_box_tag :log_description, checked: @feed[:log_description] %></td>
		    <td><%= check_box_tag :use_description_words, checked: @feed[:log_description_words] %></td>
		  </tr>
		</tbody>
	      </table>
	    </div>
          </div>

	  <div class="modal-footer">
	    <%= link_to 'Cancel', url_for(:feed, :index, page: @page), class: 'btn btn-default' %>
	    <%= f.submit 'Save', class: 'btn btn-primary' %>
	  </div>  
        <% end %>
      </div>
